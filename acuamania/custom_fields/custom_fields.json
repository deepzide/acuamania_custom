      "fieldtype": "Table MultiSelect",
      "options": "Customer__Customer_Category",
      "insert_after": "customer_name"
    },
    {
      "fieldname": "custom_qty_person",
      "label": "Cantidad de Personas",
      "fieldtype": "Int",
      "reqd": 1,
      "default": 1,
      "insert_after": "phone"
    },
    {
      "fieldname": "custom_has_hotel_voucher",
      "label": "Posee Voucher de Hotel",
      "fieldtype": "Check",
